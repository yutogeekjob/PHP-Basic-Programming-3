<?php
function prof($id){
    $a=array(1,"由比ヶ浜結衣","19940618","千葉県");
    $b=array(2,"雪ノ下雪乃","19950103",null);
    $c=array(3,"金子優斗","19950316","愛知県");
    
    if($id==1){
        return $a;
    }
    elseif ($id==2){
        return $b;
    }
    elseif($id==3){
        return $c;
    }
    else {
        return null;
    }
}
$roop=array(1,2,3);

foreach($roop as $i){
$op=prof($i);
foreach($op as $i){
    if($i==null){
      continue;
    }else{
    echo $i." ";
    }
   }
}
?>
